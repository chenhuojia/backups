/*头部*/
.top{ width: 100%; background: #333333; height: 40px; line-height: 40px;}
.top_box{ width: 1200px; margin: 0 auto; font-size: 14px; color: #999999;}
.top_box_fl{ float: left; width: 600px;}
.top_box_fr{ float: right;width: 600px; text-align: right;}
/*.top_box_fr a{ margin-left: 25px;}*/
.top_box_fl ul li{ float: left; margin-right: 30px;}
.top_box a{ color: #999999;}
.ci-left1{ background: url(../images/bg.png) no-repeat; background-position: 0px 0px; margin-top: 10px; left: 7px; width: 15px; height: 18px; display: block; float: left; margin-right: 5px;}
.ci-left2{ background: url(../images/bg.png) no-repeat;background-position: 0px -23px;  margin-top: 10px; left: 7px; width: 20px; height: 18px; display: block; float: left; margin-right: 5px;}
.ci-left3{ background: url(../images/bg.png) no-repeat;background-position: 0px -46px; margin-top: 10px; left: 7px; width: 17px; height: 16px; display: block; float: left; margin-right: 5px;}

/*header_center*/
.header_center{ width: 1200px; margin: 0 auto;}
.logo{ float: left; margin-top: 40px;}
.slogan{ float: left; margin-top: 40px; margin-left: 20px;}
.search-form{width:365px;overflow: hidden; float: left; margin-top: 40px; margin-left: 50px;}
.search-form .search-bd{height: 25px;}
.search-form .search-bd li{font-size:12px;width:60px;height: 25px;line-height: 25px;text-align:center;float: left;cursor: pointer;color: #666;}
.search-form .search-bd li.selected{color: #fff;font-weight: 700;background: #FD7A40;}
.search-form .search-hd{height:34px;background: #FD7A40;padding: 3px;position: relative;}
.search-form .search-hd .search-input{width: 280px;height: 22px;line-height: 22px;padding: 6px 0;background: none;text-indent: 10px;border: 0;outline: none;position: relative;left: 3px;top: 0;z-index: 5;#margin-left:-10px;}
.search-form .search-hd .btn-search{width: 70px;height: 34px;line-height:34px;position: absolute;right: 3px;top: 3px;border: 0;z-index: 6;cursor: pointer;font-size: 12px;color: #fff;font-weight: 700;background: none;outline: none;}
.search-form .search-hd .pholder{display: inline-block;font-size: 12px;color: #999;position: absolute;left: 13px;top: 11px;z-index: 4; background: url(../images/bg.png) no-repeat;background-position: 0px -66px;;padding-left:25px;#top:11px;}
.search-form .search-hd .s2,.search-form .search-hd #s2{display: none;}
.search-form .search-hd .s3,.search-form .search-hd #s3{display: none;}
.search-form .search-bg{width: 285px;height: 34px;background-color: #fff;position: absolute;left: 3px;top: 3px;z-index: 1;}

.cart_box{ float: right; margin-top: 65px;}
.cart_box a{ float: left; margin-left: 40px;}
.cart_link{ width: 120px; height: 40px; line-height:40px;background: #6D7E8B;font-size: 14px;color: #FFFFFF;}
.cart_link span{ margin-left: 10px}
.cart_icon1{ background: url(../images/bg.png) no-repeat;background-position: 0px -90px; margin-top: 12px; left: 7px; width: 15px; height: 14px; display: block; float: left; margin-left: 10px}
.cart_icon2{ background: url(../images/bg.png) no-repeat;background-position: 0px -110px; margin-top: 12px; left: 7px; width: 18px; height: 18px; display: block; float: left; margin-left: 10px}

/*header*/
.header{ width: 1200px; margin: 0 auto; height: 40px; line-height: 40px; background: #FD7A40; margin-top: 40px;}
.header a{ font-size: 18px;color: #FFFFFF;}
.header ul li{ float: left; width: 100px; text-align: center;}
.header_bt{ background: #DE6733;}